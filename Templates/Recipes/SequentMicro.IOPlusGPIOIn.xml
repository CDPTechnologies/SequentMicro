  <Channel Model="CDPSignalChannel&lt;bool&gt;" Name="GPIO1" Nr="0"></Channel>
  <Channel Model="CDPSignalChannel&lt;bool&gt;" Name="GPIO2"></Channel>
  <Channel Model="CDPSignalChannel&lt;bool&gt;" Name="GPIO3" Nr="2"></Channel>
  <Channel Model="CDPSignalChannel&lt;bool&gt;" Name="GPIO4" Nr="3"></Channel>

</Model>